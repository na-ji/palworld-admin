{
  "name": "palworld-admin",
  "version": "1.0.0",
  "module": "server/index.js",
  "license": "AGPL-3.0",
  "scripts": {
    "build:client": "next build",
    "check": "biome check --no-errors-on-unmatched **/*",
    "commit": "git-cz",
    "dev:client": "next dev -p 2093",
    "dev:server": "bun run --tsconfig-override tsconfig.server.json --watch server/index.ts",
    "format": "biome format --write **/*",
    "prepare": "husky",
    "start:server": "bun run --tsconfig-override tsconfig.server.json server/index.ts"
  },
  "lint-staged": {
    "*": [
      "biome check --no-errors-on-unmatched"
    ]
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@elysiajs/eden": "^0.8.1",
    "@nextui-org/react": "^2.2.9",
    "@react-stately/data": "^3.11.0",
    "@zedux/react": "^1.2.0",
    "elysia": "latest",
    "framer-motion": "^11.0.3",
    "http-proxy": "^1.18.1",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-fast-compare": "^3.2.2",
    "tslog": "^4.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/http-proxy": "^1.17.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "bun-types": "latest",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
